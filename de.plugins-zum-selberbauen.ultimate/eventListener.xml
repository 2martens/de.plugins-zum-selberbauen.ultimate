<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/eventListener.xsd">
    <import>
        <!-- PIP listener -->
        <eventlistener>
        	<environment>admin</environment>
            <eventclassname>wcf\system\package\plugin\BlockTypePackageInstallationPlugin</eventclassname>
            <eventname>install</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\PIPReplacementListener</listenerclassname>
        </eventlistener>
         <eventlistener>
         	<environment>admin</environment>
            <eventclassname>wcf\system\package\plugin\WidgetTypePackageInstallationPlugin</eventclassname>
            <eventname>install</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\PIPReplacementListener</listenerclassname>
        </eventlistener>
        <eventlistener>
            <environment>admin</environment>
            <eventclassname>wcf\system\package\plugin\BlockTypePackageInstallationPlugin</eventclassname>
            <eventname>hasUninstall</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\PIPReplacementListener</listenerclassname>
        </eventlistener>
         <eventlistener>
            <environment>admin</environment>
            <eventclassname>wcf\system\package\plugin\WidgetTypePackageInstallationPlugin</eventclassname>
            <eventname>hasUninstall</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\PIPReplacementListener</listenerclassname>
        </eventlistener>
        <eventlistener>
            <environment>admin</environment>
            <eventclassname>wcf\system\package\plugin\BlockTypePackageInstallationPlugin</eventclassname>
            <eventname>uninstall</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\PIPReplacementListener</listenerclassname>
        </eventlistener>
         <eventlistener>
            <environment>admin</environment>
            <eventclassname>wcf\system\package\plugin\WidgetTypePackageInstallationPlugin</eventclassname>
            <eventname>uninstall</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\PIPReplacementListener</listenerclassname>
        </eventlistener>
        <eventlistener>
            <environment>admin</environment>
            <eventclassname>wcf\system\package\plugin\BlockTypePackageInstallationPlugin</eventclassname>
            <eventname>findExistingItem</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\PIPReplacementListener</listenerclassname>
        </eventlistener>
         <eventlistener>
            <environment>admin</environment>
            <eventclassname>wcf\system\package\plugin\WidgetTypePackageInstallationPlugin</eventclassname>
            <eventname>findExistingItem</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\PIPReplacementListener</listenerclassname>
        </eventlistener>
        <!-- /PIP listener -->
        
        <!-- IndexFirstTime listener -->
        <eventlistener>
            <eventclassname>wcf\acp\page\IndexPage</eventclassname>
            <eventname>readData</eventname>
            <environment>admin</environment>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\IndexFirstTimeListener</listenerclassname>
        </eventlistener>
        <!-- /IndexFirstTimeListener -->
        
        <!-- block action listener -->
        <eventlistener>
            <eventclassname>ultimate\action\BlockAction</eventclassname>
            <eventname>i18nInitialization</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\BlockActionI18nListener</listenerclassname>
        </eventlistener>
        <eventlistener>
            <eventclassname>ultimate\action\BlockAction</eventclassname>
            <eventname>createBlockLoadBlockContent</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\BlockActionContentBlockTypeListener</listenerclassname>
        </eventlistener>
        <eventlistener>
            <eventclassname>ultimate\action\BlockAction</eventclassname>
            <eventname>updateBlockLoadBlockContent</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\BlockActionContentBlockTypeListener</listenerclassname>
        </eventlistener>
        <eventlistener>
            <eventclassname>ultimate\action\BlockAction</eventclassname>
            <eventname>updateBlockLoadBlockOptions</eventname>
            <inherit>0</inherit>
            <listenerclassname>ultimate\system\event\listener\BlockActionContentBlockTypeListener</listenerclassname>
        </eventlistener>
        <!-- /block action listener -->
    </import>
</data>